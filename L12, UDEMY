#include<iostream>
using namespace std;
int binarysearch(int array[],array size,int key)
int start=0;
int end=size-1;
int mid=(start+end)/2


while(start<=end){
    if(array[mid]==key){
return mid;
    }
    if(key<mid){
        end=mid-1;
    }
    else{
        start=mid+1;
    }mid=(start+end)/2;
return -1;
}
